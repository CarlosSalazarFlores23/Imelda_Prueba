@{
    ViewBag.Title = "Home Page";
}

<br />
<div class="jumbotron">
    <div class="container">
        <h2>Herramientas y utensilios para restaurantes</h2>
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <img id="caru" src="https://cdn.shopify.com/s/files/1/0469/3193/articles/utensilios_restaurante_1_prueba_1024x1024.jpg?v=1569468691" alt="uten">
                    <div class="desc">Equipa tu cocina con los mejores utensilios</div>
                </div>

                <div class="item">
                    <img id="caru" src="https://horeca.expob2b.es/uploads/fotos_noticias/2020/07/22211-174774-los-hornos-mychef-covid-free-la-solucion-mas-segura-para-restaurantes-y-hoteles.jpg" alt="horno">
                    <div class="desc">¿Lo prefieres horneado?</div>
                </div>

                <div class="item">
                    <img id="caru" src="https://reimse.mx/wp-content/uploads/2020/07/cuadro-congeladores-industriales-2-e1594774955468.jpg" alt="refri">
                    <div class="desc">Que nada se desperdicie!</div>
                </div>
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>

